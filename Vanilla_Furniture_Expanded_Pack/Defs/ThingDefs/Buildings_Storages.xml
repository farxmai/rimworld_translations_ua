  <ThingDef ParentName="LWM_DeepStorage">
    <defName>FFE_AnimalFeeder</defName>
    <label>animal feeder</label>
    <description>Small feeder for pets.</description>
    <graphicData>
      <texPath>Things/Building/AnsmalShelf/SmallAnimalShelf</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(1,1)</drawSize>
      <drawRotated>false</drawRotated>
    </graphicData>
    <staticSunShadowHeight>1</staticSunShadowHeight>
    <size>(1,1)</size>
    <statBases>
      <MaxHitPoints>10</MaxHitPoints>
      <Flammability>1</Flammability>
      <Beauty>0</Beauty>
      <Mass>1</Mass>
    </statBases>
    <pathCost>100</pathCost>
    <stuffCategories>
      <li>Woody</li>
      <li>Metallic</li>
      <li>Stony</li>
    </stuffCategories>
    <costStuffCount>5</costStuffCount>
    <building>
      <paintable>true</paintable>
      <preventDeteriorationOnTop>true</preventDeteriorationOnTop>
      <ignoreStoredThingsBeauty>true</ignoreStoredThingsBeauty>
      <fixedStorageSettings>
        <filter>
          <thingDefs>
            <li>Kibble</li>
          </thingDefs>
        </filter>
      </fixedStorageSettings>
      <defaultStorageSettings>
        <filter>
          <thingDefs>
            <li>Kibble</li>
          </thingDefs>
        </filter>
      </defaultStorageSettings>
    </building>
    <comps>
      <li Class="LWM.DeepStorage.Properties">
        <maxNumberStacks>1</maxNumberStacks>
        <minTimeStoringTakes>1</minTimeStoringTakes>
        <additionalTimeStackSize>1</additionalTimeStackSize>
        <showContents>false</showContents>
        <overlayType>SumOfAllItems</overlayType>
      </li>
    </comps>
  </ThingDef>

